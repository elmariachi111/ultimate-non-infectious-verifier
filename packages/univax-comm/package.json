{
  "name": "@univax/comm",
  "version": "0.0.97",
  "description": "univax comm is an EventSource server to communicate back to clients",
  "author": "Stefan Adolf <stefan.adolf@turbinekreuzberg.com>",
  "homepage": "https://github.com/elmariachi111/ultimate-non-infectious-verifier/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmariachi111/ultimate-non-infectious-verifier.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node build",
    "dev": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "src/index.ts",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, json",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "ts-node ./src/index.ts"
  },
  "keywords": [
    "DID",
    "Verifiable Credentials",
    "Vaccination Credentials",
    "Imunization",
    "DID Resolver"
  ],
  "devDependencies": {
    "@types/bs58": "^4.0.1",
    "@types/consolidate": "^0.14.0",
    "@types/cors": "^2.8.9",
    "@types/dotenv-flow": "^3.1.0",
    "@types/encoding-down": "^5.0.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/leveldown": "^4.0.2",
    "@types/levelup": "^4.3.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.22",
    "@types/twig": "^1.12.5",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@univax/core": "workspace:*",
    "@univax/sidetree": "workspace:*",
    "bs58": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "encoding-down": "^6.3.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "helmet": "^4.4.1",
    "leveldown": "^5.6.0",
    "levelup": "^4.4.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.0.1",
    "web3": "1.3.3"
  }
}