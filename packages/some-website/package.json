{
  "name": "some-website",
  "version": "0.0.1",
  "description": "some-website initialised with create-express-typescript-application.",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "cd build && node src",
    "dev": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, json",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "ts-node ./src/index.ts"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@types/bs58": "^4.0.1",
    "@types/consolidate": "^0.14.0",
    "@types/cors": "^2.8.9",
    "@types/dotenv-flow": "^3.1.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.22",
    "@types/twig": "^1.12.5",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@immu/core": "workspace:*",
    "@transmute/did-key-ed25519": "^0.2.1-unstable.30",
    "bs58": "^4.0.1",
    "bulma": "^0.9.1",
    "consolidate": "^0.16.0",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "express-flash-message": "^2.0.0",
    "express-session": "^1.17.1",
    "fs": "0.0.1-security",
    "helmet": "^4.4.1",
    "morgan": "^1.10.0",
    "twig": "^1.15.4",
    "twing": "^5.0.2"
  }
}
